# 서버 설정
PORT=5000
NODE_ENV=development

# 데이터베이스 설정
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=gugarden

# JWT 설정
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 세션 설정 (소셜 로그인용)
SESSION_SECRET=your_session_secret_here

# 네이버페이 설정
NAVERPAY_CLIENT_ID=your_naverpay_client_id
NAVERPAY_CLIENT_SECRET=your_naverpay_client_secret
NAVERPAY_CHAIN_ID=your_chain_id

# 네이버 소셜 로그인 설정
# https://developers.naver.com 에서 앱 등록 후 발급
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
NAVER_CALLBACK_URL=http://localhost:5000/api/auth/naver/callback

# 카카오 소셜 로그인 설정
# https://developers.kakao.com 에서 앱 등록 후 발급
KAKAO_CLIENT_ID=your_kakao_rest_api_key
KAKAO_CLIENT_SECRET=your_kakao_client_secret
KAKAO_CALLBACK_URL=http://localhost:5000/api/auth/kakao/callback

# 토스페이먼츠 설정
# https://developers.tosspayments.com 에서 테스트 키 발급
TOSS_CLIENT_KEY=test_ck_...
TOSS_SECRET_KEY=test_sk_...

# 프론트엔드 URL (CORS)
CLIENT_URL=http://localhost:5173
